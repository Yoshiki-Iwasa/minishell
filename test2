010_EnvironmentVariables

minishell$ testenv1=TEST1
minishell$ 
minishell$ 
minishell$ env | grep testenv1
minishell$ 
minishell$ 
minishell$ export testenv1
minishell$ 
minishell$ 
minishell$ env | grep testenv1
minishell$ 
minishell$ 
minishell$ export testenv2=TEST2
minishell$ 
minishell$ 
minishell$ env | grep testenv2
testenv2=TEST2
minishell$ 
minishell$ 
minishell$ echo $testenv1
TEST1
minishell$ 
minishell$ 
minishell$ echo $testenv2
TEST2
minishell$ 
minishell$ 
minishell$ echo $testenv1$testenv2
TEST1TEST2
minishell$ 
minishell$ 
minishell$ echo $testenv1 $testenv2
TEST1 TEST2
minishell$ 
minishell$ 
minishell$ echo "$testenv1"
TEST1
minishell$ 
minishell$ 
minishell$ echo "\$testenv1"
$testenv1
minishell$ 
minishell$ 
minishell$ export testenv1=TEST1
minishell$ 
minishell$ 
minishell$ export testenv1=$testenv1:add
minishell$ 
minishell$ 
minishell$ env | grep testenv1
testenv1=TEST1:add
minishell$ 
minishell$ 
minishell$ testenv1=\$value
minishell$ 
minishell$ 
minishell$ echo $testenv1
$value
minishell$ 
minishell$ 
minishell$ testenv1=$value
minishell$ 
minishell$ 
minishell$ echo $testenv1

